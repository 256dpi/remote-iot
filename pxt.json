{
    "name": "remote-iot",
    "version": "0.1.0",
    "description": "Remote IoT at Home",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "blocks.ts"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "supportedTargets": [
        "microbit",
        "calliope"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0,
                    "security_level": null,
                    "dfu_service": 0,
                    "event_service": 0,
                    "device_info_service": 0,
                    "eddystone_url": 0,
                    "eddystone_uid": 0,
                    "partial_flashing": 0
                },
                "gatt_table_size": "0x300"
            }
        }
    }
}
